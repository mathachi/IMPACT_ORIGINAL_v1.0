        "liquid": "<!DOCTYPE html>\n<html lang=\"fr\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Témoignage Client</title>\n    <style>\n        * {\n            margin: 0;\n            padding: 0;\n            box-sizing: border-box;\n        }\n\n        body {\n            font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif;\n            background: #f8f9fa;\n            margin: 0;\n            padding: 20px;\n        }\n\n        .testimonial-container {\n            background: white;\n            border-radius: 12px;\n            padding: 30px 20px;\n            text-align: center;\n            box-shadow: 0 4px 20px rgba(0, 0, 0, 0.05);\n            border: 1px solid #e2e8f0;\n            max-width: 400px;\n            width: 100%;\n            margin: 0 auto;\n            margin-bottom: -70px;\n            position: relative;\n        }\n\n        .testimonial-container::before {\n            content: '';\n            position: absolute;\n            top: -50%;\n            left: -50%;\n            width: 200%;\n            height: 200%;\n            background: radial-gradient(circle, rgba(102, 126, 234, 0.1) 0%, transparent 70%);\n            animation: rotate 20s linear infinite;\n        }\n\n        .content {\n            position: relative;\n            z-index: 2;\n        }\n\n        .title {\n            font-size: 1.25rem;\n            font-weight: 600;\n            color: #2d3748;\n            margin-bottom: 15px;\n            line-height: 1.4;\n        }\n\n        .rating-container {\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            gap: 8px;\n            margin-bottom: 0;\n        }\n\n        .rating-text {\n            font-size: 0.95rem;\n            color: #4a5568;\n            font-weight: 500;\n        }\n\n        .rating-score {\n            font-size: 1.2rem;\n            font-weight: 700;\n            color: #00d4aa;\n        }\n\n        .divider {\n            color: #a0aec0;\n            font-size: 1rem;\n            margin: 0 2px;\n        }\n\n        .stars {\n            display: flex;\n            gap: 2px;\n        }\n\n        .star {\n            font-size: 1rem;\n            color: #00d4aa;\n        }\n\n        .testimonial-container::before {\n            display: none;\n        }\n\n        .star:nth-child(1) { animation: none; }\n        .star:nth-child(2) { animation: none; }\n        .star:nth-child(3) { animation: none; }\n        .star:nth-child(4) { animation: none; }\n        .star:nth-child(5) { animation: none; }\n\n        .decorative-line {\n            display: none;\n        }\n\n        @keyframes floatUp {\n            from {\n                opacity: 0;\n                transform: translateY(30px) scale(0.95);\n            }\n            to {\n                opacity: 1;\n                transform: translateY(0) scale(1);\n            }\n        }\n\n        @keyframes sparkle {\n            0%, 100% { \n                transform: scale(1);\n                opacity: 1;\n            }\n            50% { \n                transform: scale(1.1);\n                opacity: 0.8;\n            }\n        }\n\n        @keyframes rotate {\n            from { transform: rotate(0deg); }\n            to { transform: rotate(360deg); }\n        }\n\n        @media (max-width: 480px) {\n            .testimonial-container {\n                padding: 20px 15px;\n                margin: 0 10px;\n            }\n            \n            .title {\n                font-size: 1.1rem;\n            }\n            \n            .rating-container {\n                gap: 6px;\n            }\n        }\n    </style>\n</head>\n<body>\n    <div class=\"testimonial-container\">\n        <div class=\"content\">\n            <h1 class=\"title\">Nos clients en parlent le mieux</h1>\n            \n            <div class=\"rating-container\">\n                <span class=\"rating-text\">Excellent</span>\n                <span class=\"rating-score\">4.8</span>\n                <span class=\"divider\">/</span>\n                <span class=\"rating-text\">5</span>\n                <div class=\"stars\">\n                    <span class=\"star\">★</span>\n                    <span class=\"star\">★</span>\n                    <span class=\"star\">★</span>\n                    <span class=\"star\">★</span>\n                    <span class=\"star\">★</span>\n                </div>\n            </div>\n            \n            <div class=\"decorative-line\"></div>\n        </div>\n    </div>\n</body>\n</html>",
        "background": "",
        "background_gradient": "",
        "text_color": ""
      }
